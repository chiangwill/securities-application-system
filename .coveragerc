[run]
source = .
omit =
    */migrations/*
    */venv/*
    */.venv/*
    manage.py
    */settings/*
    */tests/*
    */test_*
    */__pycache__/*
    */node_modules/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = htmlcov
